{
  "name": "web-component-disqus",
  "version": "0.0.4",
  "description": "A web component based on the Disqus React Api and is not dependent on any JavaScript Library. Just add it to your project!",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prod",
    "start": "stencil build --dev --watch --serve --prod",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^1.8.4",
    "workbox-build": "4.3.1"
  },
  "unpkg": "dist/disqus.js",
  "keywords": [
    "stencil",
    "mobile",
    "app",
    "webapp",
    "progressive web app",
    "pwa",
    "web",
    "web component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bandito11/web-component-disqus.git"
  },
  "author": "Esteban Morales",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bandito11/web-component-disqus"
  },
  "homepage": "https://github.com/Bandito11/web-component-disqus"
}
